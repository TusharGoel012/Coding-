#include <iostream>
using namespace std;

int main() {
    int N;
    cin >> N;

    int odd_sum = 0, even_sum = 0;
    bool is_odd_position = true;

    while (N > 0) {
        int digit = N % 10; // Get the last digit

        // Alternate between odd and even positions
        if (is_odd_position) {
            odd_sum += digit;
        } else {
            even_sum += digit;
        }

        // Move to the next digit
        N /= 10;
        is_odd_position = !is_odd_position; // Toggle the position
    }

    cout << odd_sum << endl;
    cout << even_sum << endl;

    return 0;
}
